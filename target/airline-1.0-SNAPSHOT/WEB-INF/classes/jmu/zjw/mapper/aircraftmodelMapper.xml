<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jmu.zjw.mapper.aircraftmodelMapper">
    <select id="findPlane" resultType="jmu.zjw.po.aircraftmodel">
        select * from aircraftmodel;
    </select>
    <insert id="insertPlane" parameterType="jmu.zjw.po.aircraftmodel">
        insert into aircraftmodel(amName) values (#{amName})
    </insert>

    <delete id="deletePlane" parameterType="int">
        delete from aircraftmodel where amId = #{amId};
    </delete>

    <select id="findPlaneByName" parameterType="String" resultType="jmu.zjw.po.aircraftmodel">
        select * from aircraftmodel where amName = #{amName}
    </select>
</mapper>